{
    "name": "lbausch/sqlgreygui",
    "description": " Web interface for SQLgrey built on Laravel 5.",
    "keywords": ["sqlgreygui", "sqlgrey", "greylisting", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "^2.5",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^2.0",
        "laravel/tinker": "~1.0",
        "lbausch/laravel-cornerstone": "^5.1",
        "league/fractal": "^0.16.0",
        "rcrowe/twigbridge": "^0.9.4",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-tail": "^1.1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~2.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^1.0",
        "laravel/homestead": "^5.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "SQLgreyGUI\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
