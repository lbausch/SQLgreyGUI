{
    "name": "lbausch/SQLgreyGUI",
    "description": " Web interface for SQLgrey built on Laravel 5.",
    "keywords": ["sqlgreygui", "sqlgrey", "greylisting", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "laravel/dusk": "^2.0",
        "laravel/framework": "5.5.*",
        "laravel/homestead": "^7.0",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "lbausch/laravel-cornerstone": "dev-develop",
        "league/fractal": "^0.17.0",
        "rcrowe/twigbridge": "^0.9.4",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-tail": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "SQLgreyGUI\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper",
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
