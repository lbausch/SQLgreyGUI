{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.0.*",
        "rcrowe/twigbridge": "0.7.x",
        "stolz/assets": "0.1.*",
        "laravelcollective/html": "~5.0",
        "components/jquery": "dev-master",
        "twbs/bootstrap": "3.3.*@dev",
        "fortawesome/font-awesome": "4.3.0",
        "afarkas/html5shiv": "dev-master",
        "rogeriopradoj/respond": "1.4.2",
        "datatables/datatables": "dev-master",
        "spatie/laravel-tail": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-debugbar": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "SQLgreyGUI\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan route:clear",
            "php artisan config:cache",
            "php artisan asset:flush --force"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "vendor"
    }
}
